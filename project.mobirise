          "_customHTML": "<section class=\"features8\" group=\"Features\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"4\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\">\n        <select title=\"Cards\" name=\"cardsAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\" selected>5</option>\n        </select>\n        <header>Color</header>\n        <input type=\"color\" title=\"Icon 1\" name=\"icon1\" value=\"#279b48\" selected>\n        <input type=\"color\" title=\"Icon 2\" name=\"icon2\" value=\"#ffb18a\" selected condition=\"cardsAmount > 1\">\n        <input type=\"color\" title=\"Icon 3\" name=\"icon3\" value=\"#6592e6\" selected condition=\"cardsAmount > 2\">\n        <input type=\"color\" title=\"Icon 4\" name=\"icon4\" value=\"#40b0bf\" selected condition=\"cardsAmount > 3\">\n        <input type=\"color\" title=\"Icon 5\" name=\"icon5\" value=\"#a8aaf1\" selected condition=\"cardsAmount > 4\">\n        <input type=\"color\" title=\"Number\" name=\"numberColor\" value=\"#1c1c25\" selected>\n        <input type=\"color\" title=\"Background\" name=\"rowbg\" value=\"#f8f8f8\" selected>\n\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@THEME_PATH@/components/_images/background3.jpg\" parallax>\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://www.youtube.com/watch?v=36YgDDJ7XSc\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container-fluid\">\n        <div class=\"row justify-content-center align-center\">\n            <div class=\"wrapper\">\n                <div class=\"card col-12 col-md-6 md-pb\" mbr-class=\"{'col-lg-6': cardsAmount == '2', 'col-lg-3': cardsAmount == '4', 'col-lg-x': cardsAmount == '5',\n                             'col-lg-4': cardsAmount == '3'}\">\n                    <div class=\"card-wrapper\">\n                        <div class=\"icon-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont icon1 mobi-mbri-contact-form mobi-mbri\"></span>\n                        </div>\n                        <div class=\"number-wrapper\">\n                            <span class=\"icon-bg number1\"></span>\n                            <a class=\"iconfont-wrapper\">\n                                <span class=\"number mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".number\">1</span>\n                            </a>\n                        </div>\n                        <div class=\"card-box\">\n                            <h4 class=\"card-title mbr-fonts-style mb-2\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-sel